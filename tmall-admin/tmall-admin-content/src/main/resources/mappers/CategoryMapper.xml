<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="top.sakai.tmall.admin.content.dao.CategoryMapper">
    <select id="getCategoryByName" resultType="top.sakai.tmall.admin.content.pojo.po.CategoryPO">
        SELECT name
        FROM tea_mall.category
    </select>
    <insert id="save">
        INSERT INTO tea_mall.category (name)
        VALUES (#{name})
    </insert>
</mapper>
